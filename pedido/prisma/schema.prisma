// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Customer {
  id String @id @default(uuid())
  externalId String
  email String @unique

  @@map("customers")
}

model Product {
  id String @id @default(uuid())
  externalId String
  code String

  @@map("products")
}